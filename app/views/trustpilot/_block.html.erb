<% if feed.is_valid? %>
    <a href="<%= feed.get_review_url %>" rel="external">
      <span class="logo"></span>
      <span class="text">We've been rated</span>
      <span class="score"><%= feed.get_human_score %></span>
      <span class="stars">
        <% (1..5).each do |i| %>
          <% if i <= feed.get_stars %>
         <span class="fullStar"></span>
          <% else %>
         <span class="emptyStar"></span>
          <% end %>
        <% end %>
      </span>
        <span class="rating">
          <span class="average"><%= feed.get_score / 10.0 %></span><span class="best"> out of 10</span>
        </span>
      <span class="footer">Click and give us your rating on Trustpilot.com</span>
    </a>
<% end %>
